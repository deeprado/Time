<!--pages/quotations/quotations.wxml-->
<view wx:for="{{array}}" wx:key="index">
  <!-- class 引用样式  -->
  <view class='item' bindtap='copy' data-item='{{item}}'>
    <block wx:if="{{item.itemType==0}}">
      <text class='title'>{{'　　'+item.english}}</text>
      <text class='content'>{{'　　'+item.chinese}}</text>
    </block>
    <block wx:if="{{item.itemType==1}}">
      <ad unit-id="adunit-7eb0ace6f41f2361"></ad>
    </block>
  </view>
</view>